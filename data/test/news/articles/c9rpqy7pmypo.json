                "type": "text",
                "model": {
                  "blocks": [
                    {
                      "type": "paragraph",
                      "model": {
                        "text": "Queen Victoria's myrtle",
                        "blocks": [
                          {
                            "type": "fragment",
                            "model": {
                              "text": "Queen Victoria's myrtle",
                              "attributes": []
                            }
                          }
                        ]
