{
  "presets": [
    [
      "env",
      {
        "useBuiltIns": true,
        "targets": {
          "browsers": [
            "chrome >= 53",
            "firefox >= 45.0",
            "ie >= 11",
            "edge >= 37",
            "safari >= 9",
            "opera >= 40",
            "op_mini >= 18",
            "Android >= 7",
            "and_chr >= 53",
            "and_ff >= 49",
            "ios_saf >= 10"
          ]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "fileName": false
      }
    ],
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ]
  ]
}
